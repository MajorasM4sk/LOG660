<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJOs.CarteCredit" table="CARTE_CREDIT" schema="EQUIPE112">
        <id name="noCarte">
            <column name="NO_CARTE" sql-type="char(16)" length="16"/>
        </id>
        <property name="typeCarte">
            <column name="TYPE_CARTE" sql-type="varchar2(30)" length="30"/>
        </property>
        <property name="anneeExpiration">
            <column name="ANNEE_EXPIRATION" sql-type="number(4)" precision="4"/>
        </property>
        <property name="moisExpiration">
            <column name="MOIS_EXPIRATION" sql-type="number(2)" precision="2"/>
        </property>
        <property name="cvv">
            <column name="CVV" sql-type="char(3)" length="3"/>
        </property>
        <property name="nomProprio">
            <column name="NOM_PROPRIO" sql-type="varchar2(50)" length="50"/>
        </property>
        <property name="idClient">
            <column name="ID_CLIENT" sql-type="number(7)" precision="7"/>
        </property>
    </class>
</hibernate-mapping>