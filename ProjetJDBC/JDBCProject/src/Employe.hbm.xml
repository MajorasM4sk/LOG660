<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJOs.Employe" table="EMPLOYE" schema="EQUIPE112">
        <id name="matricule" column="MATRICULE"/>
        <property name="courriel" column="COURRIEL"/>
        <property name="motDePasse" column="MOT_DE_PASSE"/>
        <property name="telephone" column="TELEPHONE"/>
        <property name="nom" column="NOM"/>
        <property name="prenom" column="PRENOM"/>
        <property name="adresseCivique" column="ADRESSE_CIVIQUE"/>
        <property name="dateNaissance" column="DATE_NAISSANCE"/>
    </class>
</hibernate-mapping>