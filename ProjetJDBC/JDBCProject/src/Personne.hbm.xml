<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJOs.Personne" table="PERSONNE" schema="EQUIPE112">
        <id name="idPersonne" column="ID_PERSONNE"/>
        <property name="nom" column="NOM"/>
        <property name="dateNaissance" column="DATE_NAISSANCE"/>
        <property name="lieuNaissance" column="LIEU_NAISSANCE"/>
        <property name="photo" column="PHOTO"/>
        <property name="biographie" column="BIOGRAPHIE"/>
    </class>
</hibernate-mapping>